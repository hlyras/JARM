<html>
  <head id="head">
    <% include ./../../../../partials/head %>
  </head>
  <body>
    <!-- Header -->
      <header id="header" class="alt">
        <div class='logo'>
          <a href='/'>J.A Rio militar</a>
        </div>
        <a onclick="var r = confirm('deseja sair?');if(r==true){window.location.href='/logout'}">log-out</a>
        <a href='#menu'>Menu</a>
      </header>

    <!-- Nav -->
    <% include ./../../../../partials/nav2 %>

    <!-- One -->
      <section id="One" class="wrapper style3">
        <div class="inner">
          <header class="align-center">
            <br>
            <h3>Produtos em estoque</h3>
          </header>
        </div>
      </section>

      <input type="button" class="button small alt" onclick="window.location.href='/fabrica'" value="<- voltar">

      <section id="one" class="wrapper style2">
        <div class="inner">

          <div>
            <div class="box">
              <div class="content">
                <header class="align-center">
                  <div class="row uniform">
                    <div class="4u 12u$(xsmall)">
                      <select  id="get-status" name="get-status" class="button small alt">
                        <option value="0">Não confirmado</option>
                        <option value="1">Confirmado</option>
                      </select>
                    </div>
                    <div class="4u 12u$(xsmall)">
                      <input id="get-date" name="get-date" type="date" class="button small alt">
                    </div>
                    <div class="4u 12u$(xsmall)">
                      <button id="filter-store-supply-btn" class="button medium alt">Buscar</button>
                    </div>
                    <br>
                  </div>
                  <br>
                  <table>
                    <thead>
                      <tr>
                        <td>Id</td>
                        <td>Data</td>
                        <td>Usuário</td>
                        <td>Status</td>
                        <td>Confirm.</td>
                      </tr>
                    </thead>
                    <tbody id="main-supply-tbody">
                      
                    </tbody>
                  </table>
                </header>
                <div class="align-center">
                  <br>
                  <button id="supplyPrevious" class="button small alt" autocomplete="off" disabled>&lsaquo;&lsaquo;</button>
                  <span id="supplyPageNumber"></span>
                  <button id="supplyNext" class="button small alt" autocomplete="off" disabled>&rsaquo;&rsaquo;</button>
                </div>
              </div>
            </div>
          </div>

          <div>
            <div class="box" id="showStoreSupplyBox" style="display:none;">
              <div class="content">
                <header class="align-center">
                  <div id="showStoreSupply">

                  </div>
                </header>
              </div>
            </div>
          </div>

        </div>
      </section>
    <!-- Footer -->
    <footer>
      <% include ./../../../../partials/footer %>
    </footer>
  </body>
</html>